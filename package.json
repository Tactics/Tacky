{
  "name": "@tactics/tacky",
  "version": "1.1.7",
  "description": "Tooling library for all styling related projects",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "require": "./build/index.js",
      "import": "./build/index.js",
      "types": "./build/index.d.ts"
    },
    "./storybook": {
      "require": "./build/storybook.js",
      "import": "./build/storybook.js",
      "types": "./build/storybook.d.ts"
    },
    "./tool": {
      "require": "./build/tool.js",
      "import": "./build/tool.js",
      "types": "./build/tool.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Tactics/Tacky.git"
  },
  "author": "Gert van den Buijs <gertvandenbuijs@hotmail.com>",
  "license": "MIT",
  "prepublish": "tsc",
  "scripts": {
    "format": "prettier --write src/.",
    "build" : "prettier --write src/. && ./node_modules/.bin/tsup"
  },
  "devDependencies": {
    "@types/react": "^18.2.79",
    "prettier": "2.7.1",
    "tsup": "^8.0.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.8",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4"
  }
}
